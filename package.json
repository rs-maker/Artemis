    "lint-staged": {
        "src/{main/webapp,test}/**/*.{json,js,ts,css,scss,html}": "prettier --write",
        "src/{main,test}/java/**/*.java": "bash ./linting.sh"
    },
    "scripts": {
        "postinstall": "husky install",
        "prettier:format": "prettier --write 'src/{main/webapp,test}/**/*.{json,ts,js,css,scss,html}'",
        "prettier:check": "prettier --check 'src/{main/webapp,test}/**/*.{json,ts,js,css,scss,html}'",
        "lint": "eslint . --ext .ts",
        "lint:fix": "yarn run lint --fix",
        "lint:doc": "tslint --config tslint-doc.json --project tsconfig.json -e 'node_modules/**'",
        "ngc": "ngc -p tsconfig.app.json",
        "cleanup": "rimraf build/resources/main/static/ build/resources/main/aot",
        "clean-www": "rimraf build/resources/main/static/app/{src,build/}",
        "start": "yarn run webpack:dev",
        "start-docker": "yarn run webpack:dev --hot --env.docker",
        "start-tls": "yarn run webpack:dev --hot --env.tls",
        "serve": "yarn run start",
        "build": "yarn run webpack:prod",
        "test": "node --expose-gc ./node_modules/.bin/jest --logHeapUsage --config ./src/test/javascript/jest.config.js --env=jsdom -w=4",
        "test:ci": "node --expose-gc ./node_modules/.bin/jest --logHeapUsage --config ./src/test/javascript/jest.config.ci.js --env=jsdom -w=1 --coverage",
        "test:coverage": "node --expose-gc ./node_modules/.bin/jest --logHeapUsage --config ./src/test/javascript/jest.config.js --env=jsdom -w=4 --coverage",
        "test:leaks": "jest --logHeapUsage --config=src/test/javascript/jest.config.js --env=jsdom --detectLeaks",
        "test:server-api-tests": "`pwd`/src/test/k6/api_tests.sh",
        "update": "yarn upgrade-interactive --latest",
        "webpack:dev": "yarn run webpack-dev-server --config webpack/webpack.dev.js --inline --hot --port=9060 --watch-content-base --env.stats=minimal",
        "webpack:dev-verbose": "yarn run webpack-dev-server --config webpack/webpack.dev.js --inline --hot --port=9060 --watch-content-base --profile --progress --env.stats=normal",
        "webpack:build:main": "yarn run webpack --config webpack/webpack.dev.js --env.stats=minimal",
        "webpack:build": "yarn run cleanup && yarn run webpack:build:main",
        "webpack:prod:main": "yarn run webpack --config webpack/webpack.prod.js --progress",
        "webpack:prod": "yarn run cleanup && yarn run webpack:prod:main && yarn run clean-www",
        "webpack-dev-server": "node --max-old-space-size=3072 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
        "webpack": "node --max-old-space-size=3072 --report-on-fatalerror node_modules/webpack/bin/webpack.js"
    }
